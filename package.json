{
  "name": "ugnis",
  "version": "1.0.6",
  "description": "Ugnis - design system builder",
  "author": "JulitorK",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    "browser": "./dist/esm/index.js",
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js"
  },
  "files": [
    "dist"
  ],
  "_moduleAliases": {
    "~/*": [
      "src/*"
    ]
  },
  "scripts": {
    "start": "start-storybook -p 6006",
    "build": "rm -rf dist && tsc && npx babel ./src --out-dir ./dist --extensions \".ts,.js\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "test": "jest",
    "ts": "tsc",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@emotion/react": "11.4.0",
    "@emotion/styled": "11.3.0",
    "@popperjs/core": "2.9.2",
    "@styled-system/theme-get": "5.1.2",
    "@types/lodash.mergewith": "4.6.6",
    "copy-to-clipboard": "3.3.1",
    "css-box-model": "1.2.1",
    "downshift": "6.1.3",
    "framer-motion": "4.1.17",
    "lodash.mergewith": "4.6.2",
    "react-fast-compare": "3.2.0",
    "react-icons": "4.2.0",
    "react-remove-scroll": "2.4.3",
    "react-virtuoso": "1.10.0",
    "styled-system": "5.1.5"
  },
  "devDependencies": {
    "@babel/cli": "7.14.8",
    "@babel/core": "7.14.8",
    "@emotion/jest": "11.3.0",
    "@jest/types": "27.0.6",
    "@storybook/addon-a11y": "6.3.5",
    "@storybook/addon-docs": "6.3.5",
    "@storybook/react": "6.3.5",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@testing-library/react-hooks": "7.0.1",
    "@testing-library/user-event": "13.2.0",
    "@types/chai": "4.2.21",
    "@types/faker": "5.5.7",
    "@types/jest": "26.0.24",
    "@types/jest-axe": "3.5.2",
    "@types/lodash.mergewith": "4.6.6",
    "@types/react": "17.0.14",
    "@types/react-dom": "17.0.9",
    "@types/react-frame-component": "4.1.2",
    "@types/styled-system": "5.1.12",
    "@types/styled-system__theme-get": "5.0.1",
    "@types/tinycolor2": "1.4.3",
    "@web/test-runner": "0.13.15",
    "axe-core": "4.3.1",
    "babel-loader": "8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "chai": "4.3.4",
    "csstype": "3.0.8",
    "faker": "5.5.3",
    "formik": "2.2.9",
    "jest": "27.0.6",
    "jest-axe": "5.0.1",
    "jest-environment-jsdom": "27.0.6",
    "jest-matchmedia-mock": "1.1.0",
    "jest-watch-typeahead": "0.6.4",
    "prettier": "2.3.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-frame-component": "5.1.0",
    "react-hook-form": "7.11.1",
    "react-lorem-component": "0.13.0",
    "storybook-addon-performance": "0.16.0",
    "tinycolor2": "1.4.2",
    "ts-jest": "27.0.4",
    "tsconfig-paths-webpack-plugin": "3.5.1",
    "typescript": "4.3.5"
  },
  "prettier": {
    "endOfLine": "crlf",
    "tabWidth": 2,
    "trailingComma": "all",
    "semi": false,
    "printWidth": 120
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest/dist"
    },
    "moduleNameMapper": {
      "~/(.*)": "<rootDir>/src/$1"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  }
}
